{
  "baseBranches": [
    "main"
  ],
  "extends": [
    "github>donmahallem/renovate-config"
  ],
  "packageRules": [
    {
      "groupName": "axios/turbo update {{currentVersion}}",
      "matchUpdateTypes": [
        "major"
      ],
      "matchPackageNames": [
        "/^axios$/",
        "/^@donmahallem/turbo/"
      ]
    }
  ]
}
